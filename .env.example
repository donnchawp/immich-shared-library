# Database (same as your Immich .env)
DB_PASSWORD=postgres

# Admin API key from Immich (Settings -> API Keys)
FACE_SYNC_API_KEY=

# User A's Immich user ID (the uploader)
SOURCE_USER_ID=

# User B's Immich user ID (the share recipient)
TARGET_USER_ID=

# User B's external library ID (created in Immich, auto-scan disabled)
TARGET_LIBRARY_ID=

# Path prefix that identifies shared assets in User A's library
# e.g., /external_library/user_a/
SHARED_PATH_PREFIX=

# Path prefix for User B's library (assets are remapped to this path)
# e.g., /external_library/user_b/
TARGET_PATH_PREFIX=
